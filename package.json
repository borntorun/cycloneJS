{
  "name": "cyclonejs",
  "version": "1.0.1",
  "description": "A pure-javascript adaptation of the W3C's structured cloning algorithm, designed to provide an easy interface for deep copying of complex objects",
  "main": "cyclone.js",
  "scripts": {
    "pretest": "./node_modules/.bin/jshint *.js test/*.js",
    "test": "./node_modules/.bin/mocha --reporter spec test/*.mspec.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/traviskaufman/cycloneJS.git"
  },
  "keywords": [
    "cloning",
    "copying",
    "utility",
    "objects"
  ],
  "author": "Travis Kaufman <travis.kaufman@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "mocha": "~1.12.0",
    "expect.js": "~0.2.0",
    "jshint": "~2.1.4",
    "docco": "~0.6.2"
  },
  "testling": {
    "harness": "mocha",
    "files": "test/cyclone.mspec.js",
    "browsers": {
      "ie": [
        9,
        10
      ],
      "ff": [
        18,
        19
      ],
      "chrome": [
        23,
        24,
        25
      ],
      "opera": [
        12
      ],
      "safari": [
        "5.0.5",
        5.1,
        6
      ],
      "iphone": [
        6
      ],
      "android-browser": [
        4.2
      ]
    }
  }
}
